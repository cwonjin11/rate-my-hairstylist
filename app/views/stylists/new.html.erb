<h1>Stylists#new</h1>
<p>Find me in app/views/stylists/new.html.erb</p>



  
<%= form_for @stylist do |f| %>
        <%= f.label :name %>
        <%= f.text_field :name %><br>
        <%= f.label :phone %>
        <%= f.text_field :phone %><br>
        <%= f.label :shop_name %>
        <%= f.text_field :shop_name %><br>
        <%= f.label :address %>
        <%= f.text_field :address %><br>

<%= f.submit "Create a new Hair Stylist!" %>
<% end %>